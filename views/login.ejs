<%- include('./partials/header')%>

<form method="POST" action="/users/login">
  <div class="form-group">
    <label for="exampleInputUsername1">Email address</label>
    <input type="text" class="form-control input-form" id="exampleInputEmail1" placeholder="Username" name="username">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control input-form" id="exampleInputPassword1" placeholder="Password" name="password">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>

